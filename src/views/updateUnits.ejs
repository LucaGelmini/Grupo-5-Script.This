<!DOCTYPE html>
<html lang="en">
    <head>
        <%-include("./partials/head",{title:'Login'})%>
    </head>
    <body>
        <header class="header-login">
            <%-include("./partials/header")%>
        </header>
        <form action="/units/editar" method="POST" >

        <main class="main-units-list">
            <ul class="main-container-units-list">
                <% for(let i=0; i< unidades.length;i++){%>
                    <div style="display:flex;">
                        <label for="" style="margin: 2px;">Unidad: </label>
                        <input value="<%=unidades[i].dataValues.type%>" name="<%=unidades[i].dataValues.type%>" ></input >
                    </div>
                
                <%}%>
                <button type="submit">Editar</button>

            </ul>
        </main>
    </form>

        <%-include('./partials/footer')  %> 
    </body>
</html>